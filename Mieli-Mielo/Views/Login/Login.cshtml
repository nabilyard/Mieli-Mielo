
@model Mieli_Mielo.Models.Login

@{
    ViewBag.Title = "Login";
    
}

<h2>Login</h2>

@using (Html.BeginForm("Login", "Login"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.User_Login)
        @Html.TextBoxFor(m => m.User_Login, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.MotDePasse)
        @Html.TextBoxFor(m => m.MotDePasse, new { @class = "form-control" })
    </div>
}